# Root .env.example - Copy to .env for Docker Compose

# Azure Service Bus
AZURE_SERVICE_BUS_CONNECTION_STRING=your_service_bus_connection_string

# Azure Cosmos DB
COSMOS_DB_CONNECTION_STRING=your_cosmos_db_connection_string

# Azure Storage
AZURE_STORAGE_ACCOUNT_NAME=your_storage_account_name
